{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [">1% in CN", "last 2 versions", "not ie <= 9"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx", 
    "transform-runtime",
    ["component", {
      "libraryName": "element-ui",
      "styleLibraryName": "theme-chalk"
    }]
  ],
  "env": {
    "development":{
      "plugins": ["dynamic-import-node"]
    }
  }
}
